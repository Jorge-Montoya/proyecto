<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Humo">
    <TextView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:text="Sensor Humo"
        android:textSize="30dp"
        android:textColor="@color/black"
        android:background="#936E91"
        android:layout_marginTop="20dp"
        android:id="@+id/titulosensor3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/linears3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <LinearLayout
        android:layout_width="370dp"
        android:layout_height="400dp"
        android:id="@+id/linears3"
        android:background="#8A97C3"
        android:orientation="vertical"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/titulosensor3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">
        <TextView
            android:layout_width="350dp"
            android:layout_height="185dp"
            android:id="@+id/elfeed_id4"
            android:background="#A575A8"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            />
        <TextView
            android:layout_width="350dp"
            android:layout_height="185dp"
            android:id="@+id/valor4"
            android:background="#A575A8"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>